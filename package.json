{
  "name": "@capillarytech/cap-ui-library",
  "version": "2.5.6",
  "description": "Cap HOCs for capillary style guid and antd components",
  "main": "index.js",
  "author": "meharaj",
  "repository": "https://github.com/Capillary/cap-ui-library.git",
  "private": true,
  "homepage": "/ui-library-v2/",
  "dependencies": {
    "@antv/layout": "0.1.3",
    "@antv/x6": "1.12.4",
    "@antv/x6-react-shape": "1.2.3",
    "@babel/core": "7.1.6",
    "@capillarytech/cap-ui-utils": "1.2.6",
    "@svgr/cli": "^4.1.0",
    "@svgr/core": "^4.1.0",
    "@svgr/webpack": "2.4.1",
    "antd": "3.22.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-named-asset-import": "^0.3.0",
    "babel-preset-react-app": "^7.0.0",
    "bfj": "6.1.1",
    "bizcharts": "^3.5.5",
    "cap-dagre": "0.8.6-pre",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "5.6.0",
    "eslint-config-react-app": "^3.0.6",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "exports-loader": "^0.7.0",
    "file-loader": "2.0.0",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "7.0.0",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "less-vars-to-js": "^1.3.0",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "0.4.3",
    "nanoid": "3.1.30",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "plop": "^2.2.0",
    "pnp-webpack-plugin": "1.1.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.3.1",
    "postcss-safe-parser": "4.0.1",
    "rangy": "^1.3.0",
    "react": "^16.8.1",
    "react-app-polyfill": "^0.2.0",
    "react-colorful": "^5.0.0",
    "react-datepicker": "^4.2.0",
    "react-dates": "^20.2.4",
    "react-dev-utils": "^7.0.1",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dom": "^16.8.1",
    "react-error-boundary": "^3.1.3",
    "react-test-renderer": "^16.7.0",
    "react-virtualized": "9.22.3",
    "resolve": "1.8.1",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "styled-components": "^4.1.3",
    "terser-webpack-plugin": "1.1.0",
    "url-loader": "1.1.1",
    "uuid": "8.3.2",
    "webpack": "4.19.1",
    "webpack-dev-server": "3.1.14",
    "webpack-manifest-plugin": "2.0.4",
    "whatwg-fetch": "3.0.0",
    "workbox-webpack-plugin": "3.6.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build:docs": "node scripts/build.js",
    "test:clean": "rimraf ./coverage",
    "test": "jest --config ./config/jest/jest.config.js",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "test:coverage": "jest --coverage --colors --watchAll",
    "build": "npm run build:copy-all-files",
    "build:copy-all-files": "node scripts/copy-all-files.js",
    "generate": "plop --plopfile scripts/generators/index.js",
    "lint": "npm run lint:js",
    "lint:css": "stylelint './app/**/*.js'",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged"
  },
  "jestSonar": {
    "reportPath": "reports/tests",
    "reportFile": "test-reporter.xml",
    "indent": 2
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "components/**/tests/*.js"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/test-setup.js",
    "testMatch": [
      "<rootDir>/components/**/tests/*.js"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "devDependencies": {
    "babel-plugin-import": "^1.11.0",
    "classnames": "^2.2.6",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "3.3.4",
    "eslint-config-airbnb": "^17.1.0",
    "jest": "^24.0.0",
    "jest-pnp-resolver": "1.0.1",
    "jest-resolve": "23.6.0",
    "jest-sonar-reporter": "^1.3.0",
    "lint-staged": "7.3.0",
    "pre-commit": "1.2.2",
    "prop-types": "^15.6.2"
  }
}
